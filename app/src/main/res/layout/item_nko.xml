<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:paddingStart="@dimen/dimen_20_dp"
    android:paddingTop="@dimen/dimen_17_dp"
    android:paddingEnd="@dimen/dimen_8_dp"
    android:paddingBottom="@dimen/dimen_17_dp">

    <TextView
        android:id="@+id/nkoName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/roboto_regular"
        android:singleLine="true"
        android:textColor="@color/black_87"
        android:textSize="@dimen/dimen_16_sp"
        app:drawableEndCompat="@drawable/ic_baseline_keyboard_arrow_right_24"
        app:drawableTint="@color/warm_grey"
        tools:text="@string/random_fond_name" />

</FrameLayout>
